{
    "name": "vscode-imarkdown-tm-grammar",
    "displayName": "imd",
    "description": "A syntax highlighter for a proposed IMarkdown spec",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "imarkdown",
            "aliases": ["IMarkdown", "imarkdown"],
            "extensions": [".imd",".imarkdown"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "imarkdown",
            "scopeName": "source.imarkdown",
            "path": "./syntaxes/imarkdown.tmLanguage.json"
        }]
    }
}