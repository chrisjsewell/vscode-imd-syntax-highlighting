{
    "name": "vscode-imarkdown-tm-grammar",
    "displayName": "IMarkdown",
    "description": "A syntax highlighter for a proposed IMarkdown spec",
    "version": "0.3.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "publisher": "chrisjsewell",
    "authors": [
        "Chris Sewell"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisjsewell/vscode-imd-syntax-highlighting.git"
    },
    "homepage": "https://github.com/chrisjsewell/vscode-imd-syntax-highlighting/blob/master/README.md",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "imarkdown",
                "aliases": [
                    "IMarkdown",
                    "imarkdown"
                ],
                "extensions": [
                    ".imd",
                    ".imarkdown"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "imarkdown",
                "scopeName": "text.html.imarkdown",
                "path": "./syntaxes/imarkdown.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.13.1",
        "plist": "^3.0.6",
        "vscode": "^1.1.36"
    }
}
